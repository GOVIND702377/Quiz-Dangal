###############################################
# Frontend environment (safe to expose)
# Copy this file to .env and fill values for local dev builds.
# Do NOT put server-only secrets here.
###############################################

# Supabase (client)
VITE_SUPABASE_URL=https://YOUR_SUPABASE_PROJECT.supabase.co
VITE_SUPABASE_ANON_KEY=YOUR_SUPABASE_ANON_KEY

# Web Push (VAPID public key for browser)
VITE_VAPID_PUBLIC_KEY=YOUR_VAPID_PUBLIC_KEY_BASE64

# Optional: bypass auth locally (UI only)
# 0 = disabled (default), 1 = enabled
# VITE_BYPASS_AUTH=0

# Optional: enable Supabase Realtime subscriptions
# 1 = enabled (default), 0 = disabled (falls back to polling)
# VITE_ENABLE_REALTIME=1
